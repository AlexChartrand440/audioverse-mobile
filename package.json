{
  "name": "audioverse-mobile",
  "version": "3.4.1",
  "private": true,
  "scripts": {
    "postinstall": "node ./fix_metro_android_release_bug.js && npx patch-package && npx jetify",
    "start": "react-native start --reset-cache && react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tsc --noEmit && eslint --ext .js,.json,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.json,.ts,.tsx --fix ./src",
    "prettier:write": "npx prettier --write **/*.{js,ts,tsx,json,graphql} && npx prettier --write *.{js,tsx,json}",
    "postversion": "react-native-version",
    "build:bump": "react-native-version -b",
    "ios:beta": "fastlane ios beta",
    "android:beta": "fastlane android beta",
    "graphql:codegen": "graphql codegen",
    "graphql:coverage": "graphql coverage",
    "graphql:validate": "graphql validate --noStrictFragments",
    "graphql:validate:local": "graphql-inspector validate --noStrictFragments './**/*.graphql' http://localhost:3000/graphql"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^1.1.2",
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.7",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-community/viewpager": "^3.3.1",
    "dayjs": "^1.8.36",
    "i18n-js": "^3.7.1",
    "lodash.throttle": "^4.1.1",
    "lodash.uniqby": "^4.7.0",
    "randomcolor": "^0.6.2",
    "react": "16.13.1",
    "react-native": "^0.63.2",
    "react-native-action-sheet": "^2.2.0",
    "react-native-elements": "2.2.1",
    "react-native-fbsdk": "^1.1.2",
    "react-native-firebase": "^5.6.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-localize": "^1.4.1",
    "react-native-marquee": "git+https://github.com/kyo504/react-native-marquee.git",
    "react-native-reanimated": "^1.13.0",
    "react-native-render-html": "^4.2.3",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-share": "^3.8.3",
    "react-native-simple-toast": "^1.1.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-swiper": "1.5.14",
    "react-native-tab-view": "^2.15.1",
    "react-native-track-player": "^2.0.0-rc13",
    "react-native-vector-icons": "6.6.0",
    "react-native-video": "^5.0.2",
    "react-native-video-controls": "^2.6.0",
    "react-native-webview": "^10.9.0",
    "react-navigation": "^4.4.0",
    "react-navigation-addon-search-layout": "git+https://github.com/alangumer/search-layout.git#patch-1",
    "react-navigation-stack": "^2.8.2",
    "react-navigation-tabs": "^2.9.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "rn-fetch-blob": "^0.12.0",
    "scheduler": "0.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/runtime": "^7.11.2",
    "@graphql-cli/codegen": "^1.17.8",
    "@graphql-cli/coverage": "^2.1.0",
    "@graphql-cli/validate": "^2.1.0",
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/schema-ast": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-inspector/cli": "^2.2.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/enzyme": "^3.10.6",
    "@types/i18n-js": "^3.0.3",
    "@types/jest": "^26.0.14",
    "@types/lodash.throttle": "^4.1.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/randomcolor": "^0.5.5",
    "@types/react": "^16.9.49",
    "@types/react-native": "^0.63.20",
    "@types/react-native-fbsdk": "^1.1.0",
    "@types/react-native-share": "^3.3.0",
    "@types/react-native-snap-carousel": "^3.8.2",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.3.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-native": "^3.9.1",
    "graphql": "^15.3.0",
    "graphql-cli": "^4.0.0",
    "jest": "^26.4.2",
    "metro-react-native-babel-preset": "^0.60.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.13.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.(graphql|gql)$": "<rootDir>/gqlFileTransformer.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.ts",
      "<rootDir>/__mocks__/react-native-track-player",
      "<rootDir>/__mocks__/react-native-localize",
      "<rootDir>/__mocks__/i18n-js",
      "<rootDir>/__mocks__/RNCNetInfo"
    ],
    "moduleNameMapper": {
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "<rootDir>/RelativeImageStub.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
